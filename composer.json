{
    "name": "promopult/integra-client",
    "description": "Promopult Integra API v1 client library",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "unlicense",
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-intl": "*",
        "psr/http-message": "*",
        "psr/http-client": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0.0",
        "guzzlehttp/guzzle": "~7.0"
    },
    "authors": [
        {
            "name": "Dmitry Gladyshev",
            "email": "dgladyshev@promopult.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Promopult\\Integra\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Promopult\\Integra\\Test\\": [
                "test/"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
